<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppBaseTheme" parent="@android:style/Theme.Light" />
    <style name="AppTheme" parent="@style/AppBaseTheme" />
    <style name="enrol_recognizing">
        <item name="android:textSize">18.669983sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffc600</item>
    </style>
    <style name="enrol_fail">
        <item name="android:textSize">18.669983sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="enrol_retry">
        <item name="android:textSize">18.669983sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffc600</item>
    </style>
    <style name="mmi_dialog_style" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:background">#00000000</item>
    </style>
    <style name="mmi_them" parent="@android:style/Theme.Holo">
        <item name="android:windowNoTitle">true</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="mmi_button">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ff666666</item>
        <item name="android:background">#66353b3f</item>
    </style>
    <style name="VerifyTheme" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowBackground">@color/dialog_black_dim</item>
    </style>
    <style name="EnrollTheme" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowBackground">@color/dialog_black_dim</item>
    </style>
    <style name="AppLinkTheme" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowBackground">@color/dialog_black_dim</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
    </style>
    <style name="AppAssignTheme" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowBackground">@color/dialog_black_dim</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
    </style>
</resources>
